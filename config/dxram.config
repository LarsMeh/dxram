#interfaces
# Class for the LookupInterface - Without caching: LookupHandler, with caching: CachedLookup or CachedTreeLookup
interface.lookup=de.uniduesseldorf.dxram.core.lookup.CachedTreeLookup

#net
network.ip=127.0.0.1
network.port=22221
# Reduces reaction time at the expense of a higher cpu usage
network.highPerformance=false

#ram (1 GB = 1073741824, 4 GB = 4294967296, 10 GB = 10737418240)
ram.size=25671824

#log
log.active=true
log.directory=/Users/mschoett1/Code/DXRAM/logs/
log.parallel_buffering=false
log.primary_size=64
log.secondary_size=1024

#lookup
lookup.sleep=1
lookup.cacheSize=2000
lookup.cacheTTL=5000

#zookeeper
zookeeper.connectionString=127.0.0.1:2181
# Bitfield size: 256*1024=262144 -> minimum collision probability reached; 2048*1024=2097152 -> all NodeIDs allocatable
zookeeper.bitfieldSize=2097152

#nameservice
# NAME: String, ID: Integer
nameservice.type=NAME
# Key length in Bit
nameservice.keyLength=32

#jni lock library
lock.jni_directory=/Users/mschoett1/Code/DXRAM/dxram/jni/libJNILock.dylib

#jni console library
console.jni_directory=/Users/mschoett1/Code/DXRAM/dxram/jni/libJNIconsole.dylib
